{
  "name": "easyocr",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/easyocr/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@container/python:serve",
      "options": {
        "entrypoint": "main:app",
        "module": "uvicorn",
        "args": [
          "--reload",
          "--log-config=./logging.conf"
        ],
        "cwd": "apps/easyocr/src"
      },
      "dependsOn": [
        "install"
      ]
    },
    "install": {
      "executor": "@container/python:install",
      "options": {
        "requirements": "apps/easyocr/requirements.txt"
      },
      "dependsOn": []
    },
    "lint": {
      "executor": "@container/biomejs:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "vitest": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "dependsOn": [
        "docker-test"
      ],
      "options": {}
    },
    "vitest-local": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {}
    },
    "test": {
      "executor": "@container/helper:empty",
      "dependsOn": [
        "vitest"
      ],
      "options": {}
    },
    "test-local": {
      "executor": "@container/helper:empty",
      "dependsOn": [
        "vitest-local"
      ],
      "options": {}
    },
    "docker-test": {
      "executor": "@container/docker:test",
      "dependsOn": [],
      "options": {
        "tag": "philiplehmann/easyocr:test",
        "file": "apps/easyocr/Dockerfile",
        "platforms": [
          "amd64"
        ]
      }
    },
    "docker-push": {
      "executor": "@container/docker:build",
      "dependsOn": [],
      "options": {
        "platforms": [
          "amd64"
        ],
        "tags": [
          "philiplehmann/easyocr:auto"
        ],
        "file": "apps/easyocr/Dockerfile"
      }
    },
    "docker-run": {
      "executor": "@container/docker:run",
      "options": {
        "image": "philiplehmann/easyocr:run",
        "file": "apps/easyocr/Dockerfile"
      }
    }
  },
  "tags": []
}