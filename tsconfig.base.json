{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "esnext",
    "module": "preserve",
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "allowJs": true,
    "lib": ["ESNext"],
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "skipDefaultLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false,
    "types": ["bun"],

    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@container/binary/pdftk": ["libs/binary/pdftk/src/index.ts"],
      "@container/binary/poppler": ["libs/binary/poppler/src/index.ts"],
      "@container/binary/puppeteer": ["libs/binary/puppeteer/src/index.ts"],
      "@container/binary/tesseract": ["libs/binary/tesseract/src/index.ts"],
      "@container/binary/unoserver": ["libs/binary/unoserver/src/index.ts"],
      "@container/binary/libreoffice": ["libs/binary/libreoffice/src/index.ts"],
      "@container/biomejs": ["libs/biomejs/src/index.ts"],
      "@container/bun": ["libs/bun/src/index.ts"],
      "@container/docker": ["libs/docker/src/index.ts"],
      "@container/node": ["libs/node/src/index.ts"],
      "@container/nx": ["libs/nx/src/index.ts"],
      "@container/helper": ["libs/helper/src/index.ts"],
      "@container/http/body": ["libs/http/body/src/index.ts"],
      "@container/http/error": ["libs/http/error/src/index.ts"],
      "@container/http/multipart-form-data": ["libs/http/multipart-form-data/src/index.ts"],
      "@container/http/route": ["libs/http/route/src/index.ts"],
      "@container/http/server": ["libs/http/server/src/index.ts"],
      "@container/http/validate": ["libs/http/validate/src/index.ts"],
      "@container/stream": ["libs/stream/src/index.ts"],
      "@container/test/bun": ["libs/test/bun/src/index.ts"],
      "@container/test/node": ["libs/test/node/src/index.ts"],
      "@container/test/request": ["libs/test/request/src/index.ts"],
      "@container/test/server": ["libs/test/server/src/index.ts"],
      "@container/tsc": ["libs/tsc/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
