{
  "name": "group",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/group/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/group",
        "main": "libs/group/src/index.ts",
        "tsConfig": "libs/group/tsconfig.lib.json",
        "assets": [
          "libs/group/*.md",
          {
            "input": "./libs/group/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/group/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/group",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/group",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {}
    }
  },
  "tags": []
}